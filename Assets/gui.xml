<gui>
	<anchor pos="L+50%-100,T+50%-100">
		<widget rect="0,0,200,400" id="login">
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="login_incorrect" multiline="false" terminal_mode="false" rect="0,0,200,28" value="Error logging in!" color="#00FF0000" backgroundcolor="#33FF0000" bordercolor="#FFFF0000" hoverbordercolor="#FFFF0000" visible="false" readonly="true"></textbox>
			
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="login_email" multiline="false" terminal_mode="false" rect="0,40,200,28" value="your@email.com" placeholder="your@email.com" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF12AAAA"></textbox>
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="login_password" multiline="false" terminal_mode="false" rect="0,80,200,28" value="" placeholder="password" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF12AAAA"></textbox>
			<button font="Assets/Fonts/DroidSansMono.ttf,13" id="login_submit" rect="0,120,200,28" value="login" backgroundcolor="#331228D1" bordercolor="#FF1228D1" hoverbordercolor="#FF12AAAA" visible="true" downcolor="#FFFFFF00" upcolor="#FFFFFFFF" />
		
			<button font="Assets/Fonts/DroidSansMono.ttf,13" id="login_pass_restore" rect="0,160,200,28" value="Forgot password?" backgroundcolor="#00000000" bordercolor="#00000000" hoverbordercolor="#FF12AAAA" visible="true" downcolor="#FFFFFF00" upcolor="#FFFFFFFF" />
			<button font="Assets/Fonts/DroidSansMono.ttf,13" id="login_register" rect="0,200,200,28" value="Create new account" backgroundcolor="#00000000" bordercolor="#00000000" hoverbordercolor="#FF12AAAA" visible="true" downcolor="#FFFFFF00" upcolor="#FFFFFFFF" />
		</widget>
	</anchor>
	
	<anchor pos="L+50%-100,T+50%-120">
		<widget rect="0,0,240,400" id="register">
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="register_incorrect" multiline="false" terminal_mode="false" rect="0,0,200,28" value="Error logging in!" color="#00FF0000" backgroundcolor="#33FF0000" bordercolor="#FFFF0000" hoverbordercolor="#FFFF0000" visible="false" readonly="true"></textbox>
			
			
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="register_email" multiline="false" terminal_mode="false" rect="0,40,200,28" value="your@email.com" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF12AAAA"></textbox>
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="register_username" multiline="false" terminal_mode="false" rect="0,80,200,28" value="username" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF12AAAA"></textbox>
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="register_password" multiline="false" terminal_mode="false" rect="0,120,200,28" value="password" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF12AAAA"></textbox>
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="register_password_verify" multiline="false" terminal_mode="false" rect="0,160,200,28" value="password again" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF12AAAA"></textbox>
			<button font="Assets/Fonts/DroidSansMono.ttf,13" id="register_submit" rect="0,200,200,28" value="register" backgroundcolor="#331228D1" bordercolor="#FF1228D1" hoverbordercolor="#FF12AAAA" visible="true" downcolor="#FFFFFF00" upcolor="#FFFFFFFF" />

			<button font="Assets/Fonts/DroidSansMono.ttf,13" id="register_login" rect="0,240,200,28" value="Back to main screen" backgroundcolor="#00000000" bordercolor="#00000000" hoverbordercolor="#FF12AAAA" visible="true" downcolor="#FFFFFF00" upcolor="#FFFFFFFF" />
		</widget>
	</anchor>
	
	<!-- passRestore goes here -->
	
	<!-- lobby goes here -->
	
	<widget rect="0,0,100%,100%" id="game">
		<anchor pos="rL+50,T+50">
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="game_debug" multiline="true" terminal_mode="false" rect="0,0,400,200" value=":)" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF1228D1"></textbox>
		</anchor>
		<anchor pos="R,B">
			<canvas rect="0,0,250,250" id="game_minimap" grid="false" color="#FF0021FF" pixelsize="4" aligntogrid="false" grid_color="#FF220000" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF1228D1" readonly="true"></canvas>
		</anchor>
		
		<anchor pos="rL+50,T+50">
			<textbox font="Assets/Fonts/DroidSansMono.ttf,13" id="game_test" multiline="true" terminal_mode="false" rect="0,0,400,200" value=":)" backgroundcolor="#00000000" bordercolor="#FF1228D1" hoverbordercolor="#FF1228D1"></textbox>
		</anchor>
	</widget>
</gui>
