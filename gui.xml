<gui>
	<widget rect="0,0,100%,100%" id="game">
		<anchor pos="rL+50,T+50">
			<textbox font="/usr/share/fonts/TTF/DroidSans.ttf,14" id="game_debug" multiline="true" terminal_mode="false" rect="0,0,400,200" value=":)" backgroundcolor="#00000000" bordercolor="#1228D1" hoverbordercolor="#1228D1"></textbox>
		</anchor>
		<anchor pos="R,B">
			<canvas rect="0,0,250,250" id="game_minimap" grid="false" color="#0021FF" pixelsize="4" aligntogrid="false" grid_color="#220000" backgroundcolor="#00000000" bordercolor="#1228D1" hoverbordercolor="#1228D1" readonly="true"></canvas>
		</anchor>
		
		<anchor pos="rL+50,T+50">
			<textbox font="/usr/share/fonts/TTF/DroidSans.ttf,14" id="game_test" multiline="true" terminal_mode="false" rect="0,0,400,200" value=":)" backgroundcolor="#00000000" bordercolor="#1228D1" hoverbordercolor="#1228D1"></textbox>
		</anchor>
	</widget>
	
	<anchor pos="L+50%-100,T+50%-200">
		<widget rect="0,0,200,400" id="login">
			<textbox font="/usr/share/fonts/TTF/DroidSans.ttf,14" id="login_username" multiline="false" terminal_mode="false" rect="0,0,200,28" value="username" backgroundcolor="#00000000" bordercolor="#1228D1" hoverbordercolor="#1228D1"></textbox>
			<textbox font="/usr/share/fonts/TTF/DroidSans.ttf,14" id="login_password" multiline="false" terminal_mode="false" rect="0,40,200,28" value="password" backgroundcolor="#00000000" bordercolor="#1228D1" hoverbordercolor="#1228D1"></textbox>
			<button font="/usr/share/fonts/TTF/DroidSans.ttf,14" id="login_submit" rect="0,80,200,28" value="login" backgroundcolor="#331228D1" bordercolor="#1228D1" visible="true"/>
		</widget>
	</anchor>
</gui>
